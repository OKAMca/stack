{
  "name": "directus-flexible-content",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/directus/directus-flexible-content/src",
  "projectType": "library",
  "tags": [
    "publishable"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/directus/directus-flexible-content/**/*.{ts,tsx,js,jsx}",
          "libs/directus/directus-flexible-content/package.json",
          "libs/directus/directus-flexible-content/project.json"
        ]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/directus/directus-flexible-content"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../coverage/libs/directus/directus-flexible-content",
        "passWithNoTests": true
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/directus/directus-flexible-content"
      }
    },
    "project-dry-release": {},
    "project-release": {}
  }
}

{
  "name": "directus-lib",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/directus/directus-lib/src",
  "projectType": "library",
  "tags": [
    "publishable"
  ],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "{projectRoot}/dist/libs/directus/directus-lib"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/directus/directus-lib"
      }
    },
    "project-dry-release": {},
    "project-release": {}
  }
}
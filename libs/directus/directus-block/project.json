{
  "name": "directus-block",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/directus/directus-block/src",
  "projectType": "library",
  "tags": [
    "publishable"
  ],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "{projectRoot}/dist/libs/directus/directus-block"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/directus/directus-block/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/directus-block",
        "configDir": "libs/directus/directus-block/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/directus/directus-block"
      }
    },
    "project-dry-release": {},
    "project-release": {}
  }
}
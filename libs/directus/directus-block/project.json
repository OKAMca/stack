{
  "name": "directus-block",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/directus/directus-block/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/directus/directus-block/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/directus/directus-block",
        "tsConfig": "libs/directus/directus-block/tsconfig.lib.json",
        "project": "libs/directus/directus-block/package.json",
        "entryFile": "libs/directus/directus-block/src/index.ts",
        "external": "all",
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/directus/directus-next-component/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "project-dry-release": {},
    "project-release": {}
  }
}
{
  "name": "automation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/automation/src",
  "projectType": "library",
  "tags": [
    "publishable"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/automation",
        "main": "libs/automation/src/index.ts",
        "tsConfig": "libs/automation/tsconfig.lib.json",
        "assets": [
          "libs/automation/*.md",
          {
            "input": "./libs/automation/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/automation/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/automation",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/automation",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/automation/**/*.{ts,tsx,js,jsx}",
          "libs/automation/package.json",
          "libs/automation/project.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/libs/automation"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/libs/automation"
      }
    },
    "project-dry-release": {},
    "project-release": {}
  }
}

{
  "$schema": "https://json-schema.org/schema",
  "$id": "Component",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?",
      "default": "MyComponent"
    },
    "path": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "default": "libs/ui/src/components",
      "x-prompt": "File path from root"
    }
  },
  "required": ["name", "path"]
}
